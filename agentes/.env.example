# API Keys para Serper (Obrigatória)
SERPER_API_KEY="SuaChaveDoSerperAqui"

# Provedor de LLM (google ou openrouter)
LLM_PROVIDER="google"  # Opções: "google" ou "openrouter"

# API Keys para LLM (configurar conforme o provedor escolhido)
# Para Google Gemini (quando LLM_PROVIDER=google):
GOOGLE_API_KEY="SuaChaveDoGeminiAqui"

# Para OpenRouter (quando LLM_PROVIDER=openrouter):
OPENROUTER_API_KEY="SuaChaveDoOpenRouterAqui"

# Configurações do Modelo
# Para Google: gemini-2.0-flash, gemini-pro, gemini-1.5-flash, etc.
# Para OpenRouter: anthropic/claude-3.5-sonnet, openai/gpt-4, meta-llama/llama-3.1-70b, etc.
MODEL_NAME="gemini-2.0-flash"

# Temperatura (Range: 0.0 = preciso, 1.0 = criativo)
TEMPERATURE="0.5"